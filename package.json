    "bcryptjs": "^3.0.2",
    "dompurify": "^3.2.6",
    "jsdom": "^26.1.0",
    "next": "^15.3.5",
    "next-pwa": "^5.6.0",
    "puppeteer": "^24.15.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "redis": "^5.5.6",
