{
  "name": "thiasil",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "build-check": "npm run type-check && npm run build",
    "check-debug": "rg 'console\\.log.*DEBUG|DEBUG.*console\\.log' --type js --type ts || echo 'No DEBUG statements found âœ…'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "redis:start": "docker-compose up -d",
    "redis:stop": "docker-compose down",
    "redis:logs": "docker-compose logs -f redis",
    "redis:cli": "docker exec -it thiasil-redis redis-cli",
    "redis:setup": "./scripts/setup-redis.sh",
    "analyze": "ANALYZE=true npm run build",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
