# Thiasil Environment Variables

# PWA Development Mode
# Set to 'true' to enable PWA features in development
NEXT_PUBLIC_PWA_DEV=false

# Next.js 15 Features
# Enable React Compiler for automatic optimizations (experimental)
NEXT_REACT_COMPILER=false

# Enable performance monitoring in production
NEXT_PUBLIC_PERFORMANCE_MONITOR=false

# Analytics (if needed)
# NEXT_PUBLIC_GA_ID=your-ga-id

# API Keys (if needed)
# NEXT_PUBLIC_VAPID_PUBLIC_KEY=your-vapid-key

# Development Settings
# NODE_ENV=development
# ANALYZE=true